{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/hackrx/run",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    }
  ]
}