{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/hackrx/run",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "memory": 1024,  
      "maxDuration": 10  
    }
  }
}